{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "GW2 test",
        "version": "1.0.0",
        "description": "test",
        "author": "Alliael",
        "icon": "files/icons/IconMouseOver.png",
        "icon_gray": "files/icons/IconMouseNormal.png",
        "minimum-overwolf-version": "0.77.10.0"
    },
    "data": {
        "start_window": "MinizeIcon",
        "windows": {
