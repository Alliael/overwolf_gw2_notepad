			},
            "GlobalPanel": {
                "file": "files/globalPanel.html",
                "transparent": true,
                "resizable": true,
                "size": {
					"width": 300,
					"height": 300
				},
				"min_size": {
                    "width": 60,
                    "height": 60
                },
                "max_size": {
                    "width": 600,
                    "height": 600
                }
            }
        }
    }
}
